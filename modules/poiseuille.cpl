  POISEUILLE = STRUCTURE[REAL a,b
  ]

BOOLEAN FUNCTION isInBody(POISEUILLE this^; ARRAY(*) OF REAL coordinates) FOLLOWS
BOOLEAN FUNCTION UisInBody(POISEUILLE this^; ARRAY(*) OF REAL coordinates) FOLLOWS

MODULE poiseuille
BOOLEAN FUNCTION isInBody(POISEUILLE this^; ARRAY(*) OF REAL coordinates) 
  RESULT= coordinates(2+LO)< this.a
END isInBody

BOOLEAN FUNCTION UisInBody(POISEUILLE this^; ARRAY(*) OF REAL coordinates) 
  RESULT= coordinates(2+LO)> this.b
END UisInBody
END poiseuille
